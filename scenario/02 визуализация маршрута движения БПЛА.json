            "response_set_values": "BPLA_1.properties['oes_radius'] = BPLA_1.actions.calculate_monitoring_radius(height=BPLA_1.properties['alt'], fov_degrees=BPLA_1.properties['oes_fov'],  camera_resolution_width=BPLA_1.properties['oes_res_width'], camera_resolution_height=BPLA_1.properties['oes_res_height'],  ground_resolution_required=BPLA_1.properties['oes_res_required']); self.features['OES_BPLA_ZONE_1'].properties['radius'] = BPLA_1.properties['oes_radius'];",
