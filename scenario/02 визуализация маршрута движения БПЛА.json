            "response_set_values": "BPLA_1.properties['geo_path']['geometry']['coordinates'] = BPLA_1.actions.find_path(ship_lat=BPLA_1.properties['lat'], ship_lon=BPLA_1.properties['lon'], target_lat=self.features['AIR_ZONE_1'].coordinates[0][0], target_lon=self.features['AIR_ZONE_1'].coordinates[0][1], geo_area=Model_00.actions.filter_by_type(objects=self.features, obj_type=\"RESTRICTED_AREA\"))",
